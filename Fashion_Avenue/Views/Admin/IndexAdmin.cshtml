@{
    Layout = "_LayoutAdmin";
    ViewData["Title"] = "Dashbord";
}
<section class="main_content dashboard_part">


<div class="main_content_iner ">
<div class="container-fluid plr_30 body_white_bg pt_30">
<div class="row justify-content-center">
<div class="col-lg-12">
<div class="single_element">
<div class="quick_activity">
<div class="row">
<div class="col-12">
<div class="quick_activity_wrap">
<div class="single_quick_activity">
<h4>Total User</h4>
                                            <h3><span class="counter">@ViewData["User"] </span> </h3>
</div>
<div class="single_quick_activity">
<h4>Total Products</h4>
                                            <h3><span class="counter"> @ViewData["Product"]</span> </h3>
</div>
<div class="single_quick_activity">
                                            <h4>Total Product Like</h4>
                                            <h3> <span class="counter">@ViewData["Like"] </span> </h3>
</div>
<div class="single_quick_activity">
                                            <h4>Total Blogs</h4>
                                            <h3> <span class="counter">@ViewData["Blog"]</span> </h3>
</div>
                                        <div class="single_quick_activity">
                                            <h4>Total Blogs Comments</h4>
                                            <h3> <span class="counter">@ViewData["BlogComment"]</span> </h3>
                                        </div>
                                        <div class="single_quick_activity">
                                            <h4>Total Coupon Code</h4>
                                            <h3><span class="counter">@ViewData["Coupon"] </span> </h3>
                                        </div>
                                        <div class="single_quick_activity">
                                            <h4>Total Used Coupon Code</h4>
                                            <h3><span class="counter"> @ViewData["UsedCoupon"]</span> </h3>
                                        </div>
                                        <div class="single_quick_activity">
                                            <h4>Total Checkout</h4>
                                            <h3> <span class="counter">@ViewData["Cart"] </span> </h3>
                                        </div>
                                      
</div>
</div>
</div>
</div>
</div>
</div>
</div>
</div>
</div>

<div class="footer_part">
<div class="container-fluid">
<div class="row">
<div class="col-lg-12 col-sm-12">
<div class="footer_iner text-center">
<p>2020 © Influence - Designed by<a href="#"> Dashboard</a></p>
</div>
</div>
</div>
</div>
</div>
</section>
